{
  "permissions": {
    "allow": [
      "Bash(git *)"
    ],
    "deny": [
      "Bash(git reset --hard*)",
      "Bash(git push --force*)",
      "Bash(git push -f*)",
      "Bash(git clean -fd*)",
      "Bash(git clean -fx*)",
      "Bash(git rebase -i*)",
      "Bash(git filter-branch*)",
      "Bash(git reflog expire*)"
    ]
  }
}